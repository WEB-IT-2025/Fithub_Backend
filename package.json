{
    "name": "express-typescript-starter",
    "version": "1.0.0",
    "description": "Minimal Express + TypeScript backend",
    "main": "dist/index.js",
    "scripts": {
        "dev": "nodemon",
        "build": "rimraf ./dist && tsc && tsc-alias",
        "start": "node dist/index.js",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prettier": "prettier --check .",
        "prettier:fix": "prettier --write .",
        "check": "npm run lint && npm run prettier",
        "fix": "npm run lint:fix && npm run prettier:fix"
    },
    "author": "huynhnguyen1906",
    "license": "ISC",
    "engines": {
        "node": ">=20.18.1"
    },
    "packageManager": "npm@11.1.0",
    "dependencies": {
        "@types/jsonwebtoken": "^9.0.9",
        "axios": "^1.10.0",
        "dotenv": "^16.5.0",
        "express": "^5.0.1",
        "express-validator": "^7.2.1",
        "jsonwebtoken": "^9.0.2",
        "mysql2": "^3.14.1",
        "node-cron": "^3.0.3"
    },
    "devDependencies": {
        "@eslint/js": "^9.15.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/express": "^5.0.0",
        "@types/node": "^22.10.6",
        "@types/node-cron": "^3.0.11",
        "eslint": "^9.15.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "nodemon": "^3.1.7",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "tsc-alias": "^1.8.10",
        "tsx": "^4.19.2",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.15.0"
